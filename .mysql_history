ProfDepartment\040VARCHAR(20));
CREATE\040TABLE\040ProfLogin(\040ProfId\040INT(5)\040PRIMARY\040KEY,\040ProfName\040VARCHAR(50),\040ProfPass\040VARCHAR(10),\040ProfDepartment\040VARCHAR(20));
CREATE\040TABLE\040IaMarks(
StudUsn\040VARCHAR(20),
ProfId\040INT(5),
SubCode\040VARCHAR(10),
SubName\040VARCHAR(50),
Ia1\040INT(3),
Ia2\040INT(3),
Ia3\040INT(3),
AvgIa\040INT(3),
Scheme\040INT(5),
Sem\040INT(5),
FOREIGN\040KEY(StudUsn)\040REFERENCES\040StudentLogin(StudUsn),
FOREIGN\040KEY(ProfId)\040REFERENCES\040ProfLogin(ProfId));
CREATE\040TABLE\040IaMarks(\040StudUsn\040VARCHAR(20),\040ProfId\040INT(5),\040SubCode\040VARCHAR(10),\040SubName\040VARCHAR(50),\040Ia1\040INT(3),\040Ia2\040INT(3),\040Ia3\040INT(3),\040AvgIa\040INT(3),\040Scheme\040INT(5),\040Sem\040INT(5),\040FOREIGN\040KEY(StudUsn)\040REFERENCES\040StudentLogin(StudUsn),\040FOREIGN\040KEY(ProfId)\040REFERENCES\040ProfLogin(ProfId));
CREATE\040TABLE\040StudAttd(
StudUsn\040VARCHAR(20),
