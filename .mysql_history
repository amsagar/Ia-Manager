CREATE\040TABLE\040StudentLogin(
StudUsn\040VARCHAR(20)\040PRIMARY\040KEY,
StudName\040VARCHAR(50),
StudPass\040VARCHAR(10),
StudScheme\040INT(5));
CREATE\040TABLE\040StudentLogin(\040StudUsn\040VARCHAR(20)\040PRIMARY\040KEY,\040StudName\040VARCHAR(50),\040StudPass\040VARCHAR(10),\040StudScheme\040INT(5));
CREATE\040TABLE\040StudInfo(
StudUsn\040VARCHAR(20),
StudPh\040BIGINT,
StudEmail\040VARCHAR(30),
FOREIGN\040KEY(StudUsn)\040REFERENCES\040StudentLogin(StudUsn));
CREATE\040TABLE\040StudInfo(\040StudUsn\040VARCHAR(20),\040StudPh\040BIGINT,\040StudEmail\040VARCHAR(30),\040FOREIGN\040KEY(StudUsn)\040REFERENCES\040StudentLogin(StudUsn));
CREATE\040TABLE\040ProfLogin(
ProfId\040INT(5)\040PRIMARY\040KEY,
ProfName\040VARCHAR(50),
